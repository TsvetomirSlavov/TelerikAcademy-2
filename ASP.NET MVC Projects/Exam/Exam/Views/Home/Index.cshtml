@model IList<Exam.ViewModels.TicketViewModel>

@{
    ViewBag.Title = "Ticketing System";
}

<h1>Ticketing System</h1>
<div class="row-fluid">
    @for (int i = 0; i < Model.Count; i++)
    {
        @Html.Partial("_Ticket", Model[i])

        if (i != 0 && (i + 1) % 3 == 0 && i != Model.Count - 1)
        {
            @:</div>
            @:<div class="row-fluid">
        }
    }
</div>
